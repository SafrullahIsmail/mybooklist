{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User",
  "additionalProperties": false,
  "required": [
    "name",
    "email",
    "password"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "created_at": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    }
  }
}